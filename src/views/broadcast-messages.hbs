<div class="jumbotron">
    <p class="lead">Broadcast administration Page</p>
    <p class="label-info">Available messages</p>
    <ul class="list-group">
    {{# each messagesList as |val key|}}
        <li class="list-group-item d-flex justify-content-between align-items-center">
            <strong>{{ val.dialogue_name }}</strong> {{ val.content }}
            <span class="badge badge-primary badge-pill">
                {{ val.category }}
            </span>
        </li>
    {{/each}}
    </ul>
</div>
<div class="page-footer">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <a class="btn btn-primary" href="/" role="button">Return Home</a>
            </div>
        </div>
    </div>
</div>
