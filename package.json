{
  "name": "langroo-api",
  "version": "1.0.0",
  "description": "Langroo API",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --watch src -e * --exec babel-node -- src/server.js --copy-files",
    "lint": "eslint src/",
    "build": "babel src -d dist --copy-files",
    "preprod": "npm run build",
    "prod": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/langroo/langroo-api.git"
  },
  "keywords": [
    "API",
    "Langroo",
    "Heroku"
  ],
  "author": "Mois√©s Berenguer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/langroo/langroo-api/issues"
  },
  "homepage": "https://github.com/langroo/langroo-api#readme",
  "engines": {
    "node": "9.3.0",
    "npm": "5.6.0"
  },
  "dependencies": {
    "@babel/types": "^7.0.0-beta.36",
    "aws-sdk": "^2.266.1",
    "axios": "^0.16.2",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-inline-json": "^1.2.2",
    "babel-plugin-inline-json-import": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.11.5",
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.1",
    "co": "^4.6.0",
    "connect-multiparty": "^2.1.0",
    "cron-builder": "^0.3.0",
    "cron-parser": "^2.4.3",
    "dotenv": "^4.0.0",
    "eachasync": "^1.0.3",
    "eslint-config-zavatta": "^4.2.0",
    "eslint-plugin-import": "^1.6.1",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "follow-redirects": "^1.2.5",
    "google-auth-library": "^0.11.0",
    "googleapis": "^22.2.0",
    "hbs": "^4.0.1",
    "moment": "^2.22.1",
    "mongoose": "^4.12.2",
    "mongoose-integer": "^0.1.1",
    "mongoose-type-email": "^1.0.5",
    "morgan": "^1.9.0",
    "node-schedule": "^1.2.5",
    "node-schedule-tz": "^1.2.1-4",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "paypal-rest-sdk": "^1.8.1",
    "raven": "^2.4.2",
    "redis": "^2.8.0",
    "s3fs": "^2.5.0",
    "stripe": "^5.4.0",
    "tasktimer": "^1.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.9.1",
    "nodemon": "^1.17.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      [
        "inline-json",
        {
          "matchPattern": "config"
        }
      ]
    ],
    "ignore": "src/public/**/*.*"
  },
  "eslintConfig": {
    "extends": "zavatta",
    "rules": {
      "no-console": 0,
      "callback-return": 0,
      "camelcase": 0,
      "require-await": 0
    }
  }
}
