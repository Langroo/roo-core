{
  "extends": "airbnb-base",
  "plugins": ["import"],
  "rules": {
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "no-throw-literal": 0,
    "prefer-promise-reject-errors": 0,
    "no-param-reassign": 0,
    "object-curly-newline": 1,
    "max-len": 0,
    "consistent-return": 0,
    "no-loop-func": 0,
    "no-await-in-loop": 0,
    "max-lines": 0,
    "no-console": 0,
    "import/newline-after-import": 1,
    "no-unused-vars": 1,
    "camelcase": 0,
    "arrow-body-style": 1,
    "no-return-await": 0,
    "array-callback-return": 0,
    "no-restricted-syntax": 0,
    "no-plusplus": 0,
    "no-restricted-globals": 0,
    "global-require": 0
  }
}
